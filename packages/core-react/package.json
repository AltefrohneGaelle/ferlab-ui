    "sass": "^1.26.12",
    "typescript": "^4.0.3",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "antd": "^4.6.6",
    "react": "^16.13.1",
    "react-router-dom": "^5.2.0"
